{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "workspaceName": {
      "type": "string",
      "defaultValue": "myWorkspace",
      "metadata": {
        "description": "Name of the Azure Resource Manager workspace."
      }
    },
    "location": {
      "type": "string",
      "defaultValue": "eastus",
      "metadata": {
        "description": "Location for the Azure Resource Manager workspace."
      }
    },
    "sku": {
      "type": "string",
      "defaultValue": "PerGB2018",
      "metadata": {
        "description": "SKU for the Azure Resource Manager workspace."
      }
    }
  }
}